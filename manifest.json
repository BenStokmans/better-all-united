{
  "manifest_version": 3,
  "name": "Better All United",
  "version": "1.3.2",
  "description": "",
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://*.allunited.nl/*"],
      "js": ["dist/bundle.js"],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["dist/interceptor.js"],
      "matches": ["https://*.allunited.nl/*"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}